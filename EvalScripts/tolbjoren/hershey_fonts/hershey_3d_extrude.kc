emit(a,b,c,d)
{
glBegin(GL_LINE_LOOP);
//glBegin(GL_QUADS);
    emitVertex( prismoid[b] );
    emitVertex( prismoid[a] ); // why did a,b need swapping! :O
    emitVertex( prismoid[c] );
    emitVertex( prismoid[d] );
glEnd();
}

/*
the idea is to create a chamfered prism ?
drawing a box from p1,p2 .. :/
but making sure it will fit with the next one
from p2p3 .. ?
*/

prism(vec p0, vec p1, vec p2, vec p3, Radius)
{
    // Compute orientation vectors for the two connecting faces:
    vec n0; vecsub(n0, p1, p0); normalize(n0);
    vec n1; vecsub(n1, p2, p1); normalize(n1);
    vec n2; vecsub(n2, p3, p2); normalize(n2);

    vec u; vecadd(u,n0,n1); normalize(u);
    vec v; vecadd(v,n1,n2); normalize(v);

    // Declare scratch variables for basis vectors:
    vec i,j,k; 
    double r = Radius;

    // Compute face 1 of 2:
    vec fixedNorm; vecset(fixedNorm,0,0,1);
    j = u; i = fixedNorm; veccross(k, i, j);
    vecscalar(i,i,r); vecscalar(k,k,r);

    vecset( prismoid[0], p1.x + i.x + k.x, p1.y + i.y + k.y, p1.z + i.z + k.z );
    vecset( prismoid[1], p1.x + i.x - k.x, p1.y + i.y - k.y, p1.z + i.z - k.z );
    vecset( prismoid[2], p1.x - i.x - k.x, p1.y - i.y - k.y, p1.z - i.z - k.z );
    vecset( prismoid[3], p1.x - i.x + k.x, p1.y - i.y + k.y, p1.z - i.z + k.z );
    
    // Compute face 2 of 2:
    j = v; i = fixedNorm; veccross(k, i, j);
    vecscalar(i,i,r); vecscalar(k,k,r);
    vecset( prismoid[4], p2.x + i.x + k.x, p2.y + i.y + k.y, p2.z + i.z + k.z );
    vecset( prismoid[5], p2.x + i.x - k.x, p2.y + i.y - k.y, p2.z + i.z - k.z );
    vecset( prismoid[6], p2.x - i.x - k.x, p2.y - i.y - k.y, p2.z - i.z - k.z );
    vecset( prismoid[7], p2.x - i.x + k.x, p2.y - i.y + k.y, p2.z - i.z + k.z );
    
    // Emit the six faces of the prismoid:
    emit(0,1,3,2); emit(5,4,6,7);
    emit(4,5,0,1); emit(3,2,7,6);
    emit(0,3,4,7); emit(2,1,6,5);
}


///////////////////////////// VECTOR MATH

vecScalar( vec out, vec a, s )
{
   out.x = a.x * s;
   out.y = a.y * s;
   out.z = a.z * s;
}

vecAdd( vec out, vec a, vec b)
{
   out.x = a.x + b.x;
   out.y = a.y + b.y;
   out.z = a.z + b.z;   
}
vecSub( vec out, vec a, vec b)
{
   out.x = a.x - b.x;
   out.y = a.y - b.y;
   out.z = a.z - b.z;   
}

vecCross( vec out, vec a, vec b)
{
   out.x = a.y*b.z - a.z*b.y;
   out.y = a.z*b.x - a.x*b.z;
   out.z = a.x*b.y - a.y*b.x;
}

vecset(vec a, x,y,z){
   a.x = x; a.y = y; a.z = z;
}

vecset(vec a, vec other){
   a.x = other.x; a.y = other.y; a.z = other.z;
}

vecDot( vec a, vec b)
{
   return a.x*b.x + a.y*b.y + a.z*b.z;
}

length( vec v )
{
   return sqrt( vecdot(v,v) );
}

normalize( vec v )
{
   len = length(v);
   if ( len )
   {
      v.x /= len; v.y /= len; v.z /= len;
   }
}


