      c = a[i] - 32;
      drawChar(c);
      tx += simplex[c][1]*.9;
   }
}
drawChar(c){

   verts = simplex[c][0];
   spacing = simplex[c][1];
   
   ax = simplex[c][2];
   ay = simplex[c][3]; 
   moveto(tx+ax, ty-ay);  
   for(i=4; i<verts*2; i+=2)
   {
      ax = simplex[c][i+0];
      ay = simplex[c][i+1];

      if(ax==-1 || ay == -1) moveto(tx+simplex[c][i+2], ty-simplex[c][i+3]);
      else lineto(tx+ax, ty-ay);        
   }
   lineto(tx+simplex[c][i+0],ty-simplex[c][i+1]);
}

drawstring3D(a[10])
{
   scale = .1;
   for(i=0; i<10; i++)
   {
      c = a[i] - 32;
      if ( c == 95 ) continue; // skip space
      tx += drawChar3D(c, scale) * .8;
      //tx += simplex[c][1]*scale;
   }
}

drawChar3D(c,scale){
  verts = simplex[c][0];
  if ( verts < 2 ) return;
  spacing = simplex[c][1];
  
  tz = 0;
  radi = 1/16;
  
  vec start_p, end_p;
  start_p.x = tx + simplex[c][0+2]*scale-scale*2;
  start_p.y = ty - simplex[c][0+3]*scale+scale*2;
  start_p.z = tz;
  
  vert_count = 0;
   
  for(i=4; i<verts*2+2; i+=2)
  {
    xc = simplex[c][i+0];
    yc = simplex[c][i+1];
    
    // ax or ay == -1 means start a new line
    // usually just call MoveTo
    if(xc==-1) //|| yc
    {
      start_p.x = tx + simplex[c][i+2]*scale-scale*2;
      start_p.y = ty - simplex[c][i+3]*scale+scale*2;
      start_p.z = tz; 
    }
    else // lineTo end
    {
         end_p.x = tx + xc*scale - scale*2;
         end_p.y = ty - yc*scale + scale*2;
         end_p.z = tz;         
       
         // Create verts
         prism( start_p, start_p, end_p, end_p, radi );
         
         start_p = end_p;  
      }    
   }
	return simplex[c][1]*scale;   
}

